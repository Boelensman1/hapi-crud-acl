{
  "name": "hapi-crud-acl",
  "version": "1.0.2",
  "description": "Hapi Crud Plugin",
  "engines": {
    "node": ">= 10.13"
  },
  "main": "./lib/src/index.js",
  "types": "./lib/src/index.d.ts",
  "repository": {
    "type": "git",
    "url": "https://github.com/Boelensman1/hapi-crud-acl.git"
  },
  "devDependencies": {
    "@types/hapi__boom": "^9.0.0",
    "@types/hapi__hapi": "^20.0.5",
    "@types/jest": "^26.0.20",
    "@types/lodash": "^4.14.168",
    "@types/node": "^15.0.1",
    "@typescript-eslint/eslint-plugin": "^4.15.1",
    "@typescript-eslint/parser": "^4.15.1",
    "eslint": "^7.20.0",
    "eslint-config-prettier": "^8.0.0",
    "jest": "^26.6.3",
    "prettier": "^2.2.1",
    "rimraf": "^3.0.2",
    "shx": "^0.3.3",
    "ts-jest": "^26.5.1",
    "tsutils": "^3.20.0",
    "typescript": "^4.1.5"
  },
  "files": [
    "lib"
  ],
  "scripts": {
    "clean": "rimraf coverage lib tmp",
    "build": "tsc -p tsconfig.release.json && shx sed -i 's/\"files\":/\"_files\":/' lib/package.json",
    "build:watch": "tsc -w -p tsconfig.release.json",
    "lint": "eslint . && prettier --check .",
    "test": "jest --coverage",
    "test:watch": "jest --watch",
    "prepare": "npm run clean && npm run build",
    "prepublishOnly": "npm test && npm run lint"
  },
  "author": "Wigger Boelens",
  "license": "MIT",
  "dependencies": {
    "@hapi/boom": "^9.1.1",
    "@hapi/hapi": "^20.1.0",
    "lodash": "^4.17.21",
    "tslib": "^2.1.0"
  }
}
